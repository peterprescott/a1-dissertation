@article{MFleischmannEtAl2020b,
  title = {Measuring Urban Form: {{Overcoming}} Terminological Inconsistencies for a Quantitative and Comprehensive Morphologic Analysis of Cities},
  shorttitle = {Measuring Urban Form},
  author = {Fleischmann, Martin and Romice, Ombretta and Porta, Sergio},
