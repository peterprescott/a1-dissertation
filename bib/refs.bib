  keywords = {OpenStreetMap,Urban design,Urban form,Urban morphology,Urban planning,Visualization},
  file = {/home/peterprescott/Zotero/storage/WJLCPJGJ/Boeing - 2019 - Spatial information and the legibility of urban fo.pdf;/home/peterprescott/Zotero/storage/WIGKBFQJ/S0268401219302154.html}
